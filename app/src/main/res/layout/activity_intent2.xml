<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".intent2">

    <TextView
            android:layout_width="323dp"
            android:layout_height="0dp"
            android:id="@+id/rest" android:textSize="18sp"
            android:layout_marginTop="35dp" android:layout_marginBottom="44dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/button10" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>
    <Button
            android:text="Compartilhar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/button10"
            app:layout_constraintTop_toBottomOf="@+id/rest" android:layout_marginBottom="191dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="20dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="20dp" android:layout_marginEnd="20dp" android:layout_marginRight="20dp"/>
</android.support.constraint.ConstraintLayout>