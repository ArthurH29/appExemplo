<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Intents" tools:layout_editor_absoluteY="81dp">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:id="@+id/textView8" android:text="Modelo do Automóvel:"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/textInputLayout24"
            android:layout_marginTop="38dp" android:layout_marginBottom="9dp"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout24"/>
    <TextView
            android:text="Distância(Km):"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:id="@+id/textView11" android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textInputLayout25"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout24"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout25"/>
    <TextView
            android:text="Potência do motor:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textInputLayout26"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout25"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout26"/>
    <TextView
            android:text="Valor do litro da gasolina:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13" android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/textInputLayout27"
            app:layout_constraintBottom_toBottomOf="@+id/textInputLayout27" android:layout_marginBottom="48dp"/>
    <android.support.design.widget.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="47dp"
            android:id="@+id/textInputLayout24"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginBottom="9dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginEnd="44dp" android:layout_marginRight="44dp"
            app:layout_constraintHorizontal_bias="0.0">

        <android.support.design.widget.TextInputEditText
                android:layout_width="334dp"
                android:layout_height="40dp"
                android:id="@+id/id1"/>
    </android.support.design.widget.TextInputLayout>
    <android.support.design.widget.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:id="@+id/textInputLayout25"
            app:layout_constraintTop_toBottomOf="@+id/textView11" android:layout_marginBottom="20dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textView12"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginEnd="44dp" android:layout_marginRight="44dp">

        <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:id="@+id/id2"/>
    </android.support.design.widget.TextInputLayout>
    <android.support.design.widget.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="54dp"
            android:id="@+id/textInputLayout26"
            app:layout_constraintTop_toBottomOf="@+id/textView12" android:layout_marginBottom="59dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textInputLayout27"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginEnd="44dp" android:layout_marginRight="44dp">

        <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:id="@+id/id3"/>
    </android.support.design.widget.TextInputLayout>
    <android.support.design.widget.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:id="@+id/textInputLayout27"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout26" android:layout_marginBottom="17dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/button9"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginEnd="44dp" android:layout_marginRight="44dp">

        <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:id="@+id/id4"/>
    </android.support.design.widget.TextInputLayout>
    <Button
            android:text="Calcular"
            android:layout_width="216dp"
            android:layout_height="38dp"
            android:id="@+id/button9" android:onClick="caculoGasolina"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout27" android:layout_marginBottom="45dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>